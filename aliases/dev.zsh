# Editor
alias vim="nvim"
alias zshrc='cd ~/.config/.zsh'
alias zshs='source ~/.zshrc'
alias todo='nvim $NOTES/TODO.md'

# Development tools
alias ollama="docker exec -it ollama bash"
alias kitten='kitty +kitten themes'

# Conda
alias cload="source $SCR/conda_load.sh"

alias dev-h="cat ~/.config/.zsh/aliases/dev.zsh"
